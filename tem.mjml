            <mj-text>
              <a href="{{this.link}}">{{this.title}}</a>
            </mj-text>
            <mj-text>{{{this.content}}}</mj-text>
            <mj-divider></mj-divider>
          </mj-column>
        </mj-section>
      {{/each}}
    {{/each}}

      
     
    <mj-section background-color="#572C64" padding-top="8px">
